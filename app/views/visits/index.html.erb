
<link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css">

<link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap-theme.min.css">

<script src="//maxcdn.bootstrapcdn.com/bootstrap/3.2.0/js/bootstrap.min.js"></script>

<%= flash_message %>

<h2><%= @location.name %>
	<%= @location.city %></h2>
	<ul>
 		<% @visits.each do |visit| %>
 	<li>
 		<%= visit.user_name %>
 		<%= link_to 'KILL', location_visit_path(@location, visit), :method => :delete %></strong>
 		<%= link_to 'EDIT', edit_location_visit_path(@location, visit) %></strong>
 		<br/>From: <%= visit.from_date %>
 		<br/>To: <%= visit.to_date %>
 	</li>
	 <% end %>
	</ul>
<%= link_to 'NEW VISIT', new_location_visit_path(@location) %></strong>





